{
    
    "builds": [
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "Python 3.10" }

        } 
        
    ],
    "routes": [
        
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}